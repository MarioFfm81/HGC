%select{:onchange=>"changeMatchday(this)"}
	-(1..34).each do |i|
		-if i==@currentMatchday.to_i
			%option{:value=>"#{overview_path(i)}", :selected=>true} Spieltag #{i}
		-else
			%option{:value=>"#{overview_path(i)}"} Spieltag #{i}
	
%table{:border=>"1", :id=>"ContentTable"}
	%tr
		%td
		-@games.each do |game|
			%td{:class=>"gameHeader"}
				=image_tag "#{game['Team1']['TeamIconUrl']}"
				&#58
				=image_tag "#{game['Team2']['TeamIconUrl']}"
				<br />
				-if game['MatchResults'][0]
					{game['MatchResults'][0]['PointsTeam1']}
				&#58
				-if game['MatchResults'][0]
					{game['MatchResults'][0]['PointsTeam2']}
		%td Aktuell
		%td Bisher
		%td Gesamt
	-@users.each do |user|
		%tr
			%td{:class=>"userCol1"} #{user.username}
			-10.times do
				%td
			%td #{@resultPerUser[user.id]}
			%td
			

.content #{@games[0]}

